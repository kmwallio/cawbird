<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="AboutDialog" parent="GtkAboutDialog">
    <property name="can-focus">1</property>
    <property name="title" translatable="yes">About Cawbird</property>
    <property name="type-hint">dialog</property>
    <property name="program-name">Cawbird</property>
    <property name="version">1.1.0</property>
    <property name="website">https://ibboard.co.uk/cawbird/</property>
    <property name="authors">IBBoard &lt;cawbird@ibboard.co.uk&gt; (Cawbird)
Timm Bäder &lt;mail@baedert.org&gt; (Corebird)</property>
    <property name="artists">IBBoard &lt;cawbird@ibboard.co.uk&gt; (Cawbird icon)
Timm Bäder &lt;mail@baedert.org&gt; (Corebird graphics)
Gnome Project (Padlock icon - CC-BY-SA3 from Adwaita icon theme)</property>
    <property name="logo-icon-name">uk.co.ibboard.cawbird</property>
    <property name="license-type">gpl-3-0</property>
    <signal name="response" handler="response_cb" swapped="no"/>
  </template>
</interface>
